{
    "class": "ADC",
    "schemaVersion": "3.54.0",
    "Sample_HTTPS": {
        "class": "Tenant",
        "MyHttps": {
            "class": "Application",
            "service":
{                 "class": "Service_HTTPS",                 "virtualAddresses": [                     "192.0.2.1"                 ],                 "virtualPort": 8080,                 "clientTLS": "tlsClient",                 "serverTLS": "tlsServer"             }
,
            "tlsServer": {
                "class": "TLS_Server",
                "certificates": [
                   
{                         "certificate": "webcert"                     }
                ],
                "renegotiationEnabled": false
            },
            "tlsClient":
{                 "class": "TLS_Client",                 "clientCertificate": "webcert",                 "renegotiationEnabled": false             }
,
            "webcert": {
                "class": "Certificate",
                "certificate":
{                     "bigip": "/Common/default.crt"                 }
,
                "privateKey":
{                     "bigip": "/Common/default.key"                 }
            }
        }
    }
}