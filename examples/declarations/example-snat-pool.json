{
    "class": "ADC",
    "schemaVersion": "3.16.0",
    "label": "snat_pool_existing",
    "remark": "reference_snat_pool",
    "Sample_SNAT_Pool": {
        "class": "Tenant",
        "SNAT_app": {
            "class": "Application",
            "SNAT_service": {
                "class": "Service_TCP",
                "virtualPort": 8181,
                "virtualAddresses": [
                    "192.0.2.100"
                ],
                "pool": "web_pool",
                "snat": {
                    "use": "CreateSnatPool"
                }
            },
            "web_pool": {
               "class": "Pool",
               "monitors": [
                  "http"
               ],
               "members": [{
                  "servicePort": 80,
                  "serverAddresses": [
                     "192.0.6.10",
                     "192.0.6.11"
                  ]
               }]
            },
            "CreateSnatPool": {
                "class": "SNAT_Pool",
                "snatAddresses": [
                    "192.0.96.10",
                    "192.0.96.11"
                 ]
            }
        }
    }
}
